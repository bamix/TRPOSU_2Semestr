<Window x:Class="lab13.Dialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab13"
        mc:Ignorable="d"
        Title="Dialog" Height="154.393" Width="315.063">
    <Grid>
        <TextBox x:Name="textBlock" HorizontalAlignment="Left" Margin="34,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="92" Text="{Binding Path=Min}" PreviewTextInput="PreviewTextInput"/>
        <TextBox x:Name="textBlock1" HorizontalAlignment="Left" Margin="195,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="83" Text="{Binding Path=Max}" PreviewTextInput="PreviewTextInput"/>
        <Label x:Name="label" Content="b:" HorizontalAlignment="Left" Margin="170,42,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="a:" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left" Margin="67,93,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" IsCancel="True" Content="Cancel" HorizontalAlignment="Left" Margin="170,93,0,0" VerticalAlignment="Top" Width="75" />

    </Grid>
</Window>
